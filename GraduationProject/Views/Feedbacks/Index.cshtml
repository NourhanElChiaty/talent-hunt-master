@model GraduationProject.ViewModels.AdminViewModel


<table class="table">
    <tr>
        <th>
            UserName
        </th>
        <th>
            Massage
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Feedback) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Talented.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Message)
        </td>
        <td>
           
            @Html.ActionLink("Delete", "Delte", new { id=item.Id })
        </td>
    </tr>
}

</table>

@model GraduationProject.ViewModels.AdminViewModel

@{
    ViewBag.Title = "Index";
}



<table class="table">
    <tr>
        <th>
            UserName
        </th>
        
        <th>
            EventName
        </th>
        <th>
            Location
        </th>
        <th>
            Date
        </th>
        <th>
            From
        </th>
        <th>
            To
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model.Event) { 
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Talented.Name)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Event_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event_Location)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event_From)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event_To)
        </td>
       
        <td>
           
            @Html.ActionLink("Delete", "Dele", new { id=item.Id })
        </td>
    </tr>
}

</table>
